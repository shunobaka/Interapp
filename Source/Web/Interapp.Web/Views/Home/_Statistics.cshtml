@model Interapp.Web.ViewModels.Home.StatisticsViewModel
<h2>Statistics</h2>
<div class="row">
    <div class="col-md-3">
        <h4>Students <span class="badge">@Model.StudentsCount</span></h4>
    </div>
    <div class="col-md-3">
        <h4>Directors <span class="badge">@Model.DirectorsCount</span></h4>
    </div>
    <div class="col-md-3">
        <h4>Universities <span class="badge">@Model.UniversitiesCount</span></h4>
    </div>
    <div class="col-md-3">
        <h4>Applications <span class="badge">@Model.ApplicationsCount</span></h4>
    </div>
</div>
<br />
<h6>Statistics may be inaccurate because the data is cached for 1 hour.</h6>